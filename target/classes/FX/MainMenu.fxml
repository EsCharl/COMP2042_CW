<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="parentcontainer" stylesheets="@../CSS/MainMenu.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FX.MainMenuController">
    <VBox alignment="CENTER" prefHeight="300.0" prefWidth="450.0" spacing="10" style="-fx-background-image: ../images/mainimage.png;">
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>

        <Button fx:id="info" mnemonicParsing="false" onAction="#onInfoButtonClick" prefHeight="27.0" prefWidth="89.0" style="-fx-background-color: 20; -fx-border-color: blue;" text="INFO" />
       <Button fx:id="start" mnemonicParsing="false" onAction="#onPlayButtonClick" prefHeight="25.0" prefWidth="91.0" text="START GAME" />
       <Button fx:id="exit" mnemonicParsing="false" onAction="#onExitButton" prefHeight="25.0" prefWidth="91.0" text="Exit" />
    </VBox>
</StackPane>
